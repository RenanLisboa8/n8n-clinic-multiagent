    {
      "parameters": {
        "jsCode": "// Build events list request for Google Calendar Client\n// API: GET calendar/v3/calendars/{calendarId}/events?timeMin=...&timeMax=...\nconst p = $input.first().json;\nconst encodedCalId = encodeURIComponent(p.calendar_id);\nconst endpoint = `calendar/v3/calendars/${encodedCalId}/events?timeMin=${encodeURIComponent(p.time_min)}&timeMax=${encodeURIComponent(p.time_max)}`;\n\nreturn {\n  tenant_id: p.tenant_id,\n  method: 'GET',\n  google_api_endpoint: endpoint,\n  body: {},\n  _calendar_id: p.calendar_id\n};"
      },
      "id": "build-events-request",
      "name": "Build Events Request",
      "type": "n8n-nodes-base.code",
      "position": [680, 300],
      "typeVersion": 2,
      "notes": "Constructs GET request for calendar/v3/calendars/{id}/events"
    },
    {
