      "id": "error-response",
      "name": "Return Client Error",
      "type": "n8n-nodes-base.set",
      "position": [1340, 420],
      "typeVersion": 3.4
    },
    {
      "parameters": {
        "jsCode": "// Format events list from Google Calendar API response\n// API returns: { items: [...], nextPageToken?, ... }\nconst apiResponse = $input.first().json;\nconst params = $('Build Events Request').first().json;\nconst calendarId = params._calendar_id;\n\n// Handle client error response\nif (apiResponse.success === false || apiResponse.error) {\n  return {\n    success: false,\n    error: apiResponse.error || 'API call failed',\n    calendar_id: calendarId,\n    events: [],\n    event_count: 0\n  };\n}\n\n// Extract items array (Google API returns { items: [...] })\nconst rawEvents = apiResponse.items || [];\n\nreturn {\n  calendar_id: calendarId,\n  event_count: rawEvents.length,\n  success: true,\n  events: rawEvents.map(e => ({\n    event_id: e.id,\n    summary: e.summary,\n    start: e.start?.dateTime || e.start?.date,\n    end: e.end?.dateTime || e.end?.date,\n    description: e.description,\n    html_link: e.htmlLink\n  }))\n};"
      },
      "id": "format-events",
      "name": "Format Events",
      "type": "n8n-nodes-base.code",
      "position": [1340, 240],
      "typeVersion": 2,
      "notes": "Formats events from API response (items array)"
    }
